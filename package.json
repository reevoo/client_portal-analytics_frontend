{
  "name": "client_portal-analytics_frontend",
  "version": "0.0.0",
  "description": "React version of Client Portal analytics frontend",
  "repository": "https://github.com/reevoo/client_portal-analytics_frontend",
  "main": "index.js",
  "config": {
    "pact_port_cp_analytics_api": 10014,
    "pact_port_cp_admin_api": 10015,
    "pact_port_tableau_gateway_api": 10004
  },
  "scripts": {
    "wait_for_port": "node scripts/wait_for_port.js",
    "pact:start:cp_analytics": "bundle exec pact-mock-service start -p $npm_package_config_pact_port_cp_analytics_api -l tests/pact/log/client_portal_analytics_backend.log --pact-dir tests/pact/pacts --cors",
    "pact:start:cp_admin": "bundle exec pact-mock-service start -p $npm_package_config_pact_port_cp_admin_api -l tests/pact/log/api_client_portal-admin.log --pact-dir tests/pact/pacts --cors",
    "pact:start:tableau_gateway": "bundle exec pact-mock-service start -p $npm_package_config_pact_port_tableau_gateway_api -l tests/pact/log/tableau_api_gateway.log --pact-dir tests/pact/pacts --cors",
    "pact:start": "npm run pact:start:cp_analytics ; npm run pact:start:cp_admin ; npm run pact:start:tableau_gateway",
    "pact:stop:cp_analytics": "bundle exec pact-mock-service stop -p $npm_package_config_pact_port_cp_analytics_api",
    "pact:stop:cp_admin": "bundle exec pact-mock-service stop -p $npm_package_config_pact_port_cp_admin_api",
    "pact:stop:tableau_gateway": "bundle exec pact-mock-service stop -p $npm_package_config_pact_port_tableau_gateway_api",
    "pact:stop": "npm run pact:stop:cp_analytics ; npm run pact:stop:cp_admin ; npm run pact:stop:tableau_gateway",
    "pact:setup:cp_analytics": "node tests/pact/setup/setup.js client_portal_analytics_backend",
    "pact:setup:cp_admin": "node tests/pact/setup/setup.js api_client_portal-admin",
    "pact:setup:tableau_gateway": "node tests/pact/setup/setup.js tableau_api_gateway",
    "pact:setup": "npm run pact:setup:cp_analytics ; npm run pact:setup:cp_admin ; npm run pact:setup:tableau_gateway",
    "pact:teardown:cp_analytics": "node tests/pact/setup/teardown.js client_portal_analytics_backend",
    "pact:teardown:cp_admin": "node tests/pact/setup/teardown.js api_client_portal-admin",
    "pact:teardown:tableau_gateway": "node tests/pact/setup/teardown.js tableau_api_gateway",
    "pact:teardown": "npm run pact:teardown:cp_analytics ; npm run pact:teardown:cp_admin ; npm run pact:teardown:tableau_gateway",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "ci": "TEST_ENV=ci npm run test && npm run build",
    "start": "webpack-dev-server",
    "test:contract": "npm run pact:start && npm run wait_for_port -- -p $npm_package_config_pact_port_cp_analytics_api,$npm_package_config_pact_port_cp_admin_api,$npm_package_config_pact_port_tableau_gateway_api && npm run pact:setup && karma start tests/karma.conf.contract.js && npm run pact:teardown && npm run pact:stop",
    "test:unit": "karma start tests/karma.conf.unit.js",
    "test:unit:watch": "karma start tests/karma.conf.unit.js --autoWatch=true --singleRun=false",
    "test": "npm run test:unit && npm run test:contract",
    "invalidate": "node scripts/cloudfront_invalidate.js",
    "deploy:staging": "NODE_ENV=staging npm run build && aws s3 sync dist s3://client-portal-staging/analytics --acl=public-read && NODE_ENV=staging npm run invalidate",
    "deploy:production": "NODE_ENV=production npm run build && aws s3 sync dist s3://client-portal-production/analytics --acl=public-read --cache-control=60 && NODE_ENV=production npm run invalidate"
  },
  "author": "Alexandru Anca and Ed Saunders",
  "license": "ISC",
  "dependencies": {
    "client_portal-assets": "^0.4.0",
    "js-cookie": "^2.1.2",
    "material-ui": "^0.15.3",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.12",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-jsx": "^1.2.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.1.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.8.0",
    "pact-consumer-js-dsl": "^0.2.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "redux-mock-store": "^1.0.2",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "sass-variable-loader": "0.0.3",
    "style-loader": "^0.13.1",
    "tcp-port-used": "^0.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
}
